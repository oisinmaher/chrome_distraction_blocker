{
"manifest_version": 3,
"name": "DistractionBlocker",
"version": "1.0.0",
"content_scripts": [
    {
        "matches" : ["https://www.youtube.com/*", "https://www.tiktok.com/*", "https://x.com/*", "https://www.facebook.com/*", "https://www.instagram.com/*"],
        "js" : ["Content.js"],
        "run_at" : "document_end"
    }
]
}